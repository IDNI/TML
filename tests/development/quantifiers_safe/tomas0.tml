Axy0(?w ?z) :- forall ?x {{?x <= 2}  -> exists ?y { p(?x ?y)}} && {?w <= 2} && p(?w ?z).
Axy1(?w ?z) :- forall ?x {{?x <  3}  -> exists ?y { p(?x ?y)}} && {?w <  3} && p(?w ?z).
Axy2(?w ?z) :- forall ?x { ~{ ?x >= 3 } -> exists ?y { p(?x ?y) } } && ~{ ?w >= 3 } && p(?w ?z).
Bxy0(?w ?z) :- forall ?x { {?x<=2} -> exists ?y { { p(?x ?y)} } } && {?w<=2} && p(?w ?z).
Bxy1(?w ?z) :- forall ?x { ~{3<?x} -> exists ?y { p(?x ?y) } } && ~{3<?w} && p(?w ?z).
Bxy2(?w ?z) :- forall ?x {~{3>=?x} -> exists ?y { p(?x ?y)} } && ~{3>=?w} && p(?w ?z).
